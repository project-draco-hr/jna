{
  HRESULT hrCI=Ole32.INSTANCE.CoInitializeEx(null,0);
  GUID guid=Ole32Util.getGUIDFromString("{13709620-C279-11CE-A49E-444553540000}");
  GUID riid=Ole32Util.getGUIDFromString("{D8F015C0-C278-11CE-A49E-444553540000}");
  PointerByReference iUnknown=new PointerByReference();
  final int CLSCTX_ALL=(WTypes.CLSCTX_INPROC_SERVER | WTypes.CLSCTX_INPROC_HANDLER | WTypes.CLSCTX_LOCAL_SERVER| WTypes.CLSCTX_REMOTE_SERVER);
  HRESULT hr=Ole32.INSTANCE.CoCreateInstance(guid,null,CLSCTX_ALL,riid,iUnknown);
  assertTrue(W32Errors.SUCCEEDED(hr.intValue()));
  assertTrue(!iUnknown.getValue().equals(Pointer.NULL));
  if (W32Errors.SUCCEEDED(hrCI.intValue()))   Ole32.INSTANCE.CoUninitialize();
}
