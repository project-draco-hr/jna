{
  WinBase.STARTUPINFO startupInfo=new WinBase.STARTUPINFO();
  WinBase.PROCESS_INFORMATION.ByReference processInformation=new WinBase.PROCESS_INFORMATION.ByReference();
  boolean status=Kernel32.INSTANCE.CreateProcessW(null,Native.toCharArray("cmd.exe /c echo hi"),null,null,true,new WinDef.DWORD(0),Pointer.NULL,System.getProperty("java.io.tmpdir"),startupInfo,processInformation);
  assertTrue(status);
  assertTrue(processInformation.dwProcessId.longValue() > 0);
}
