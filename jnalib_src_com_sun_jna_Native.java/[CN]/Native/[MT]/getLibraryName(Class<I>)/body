{
  NativeLibrary annotation=interfaceClass.getAnnotation(NativeLibrary.class);
  if (annotation == null) {
    throw new IllegalArgumentException("Library interface must be " + "annotated with the @" + NativeLibrary.class.getName() + " annotation");
  }
  String name=annotation.name();
  if ("".equals(name)) {
    throw new IllegalArgumentException("No value for the name " + "attribute was specified on the @" + NativeLibrary.class.getName() + " annotation");
  }
  return name;
}
