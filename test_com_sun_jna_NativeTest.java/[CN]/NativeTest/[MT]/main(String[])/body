{
  try {
    if (args.length == 0) {
      junit.textui.TestRunner.run(NativeTest.class);
    }
 else {
      junit.textui.TestRunner.run(Class.forName(args[0]));
    }
  }
 catch (  Throwable t) {
    System.err.println("Error in test: ");
    t.printStackTrace();
  }
  try {
    Thread.sleep(300000);
  }
 catch (  Exception e) {
  }
}
