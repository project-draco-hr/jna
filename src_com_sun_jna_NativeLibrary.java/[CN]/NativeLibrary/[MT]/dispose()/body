{
synchronized (libraries) {
    libraries.remove(getName() + options);
    File file=getFile();
    if (file != null) {
      libraries.remove(file.getAbsolutePath() + options);
      libraries.remove(file.getName() + options);
    }
  }
synchronized (this) {
    if (handle != 0) {
      Native.close(handle);
      handle=0;
    }
  }
}
