{
  String value=System.getProperty(key,"");
  if ("".equals(value)) {
    return Collections.EMPTY_LIST;
  }
  StringTokenizer st=new StringTokenizer(value,File.pathSeparator);
  List list=new ArrayList();
  while (st.hasMoreTokens()) {
    String path=st.nextToken();
    if (!"".equals(path)) {
      list.add(path);
    }
  }
  return list;
}
