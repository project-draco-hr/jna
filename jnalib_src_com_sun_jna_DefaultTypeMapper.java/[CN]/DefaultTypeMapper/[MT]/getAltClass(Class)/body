 else   if (cls == long.class) {
    return Long.class;
  }
 else   if (cls == Float.class) {
    return float.class;
  }
 else   if (cls == float.class) {
    return Float.class;
  }
 else   if (cls == Double.class) {
    return double.class;
  }
 else   if (cls == double.class) {
    return Double.class;
  }
