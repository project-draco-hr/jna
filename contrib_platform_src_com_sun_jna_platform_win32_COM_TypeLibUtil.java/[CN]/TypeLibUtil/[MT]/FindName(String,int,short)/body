{
  BSTRByReference szNameBuf=new BSTRByReference(OleAuto.INSTANCE.SysAllocString(name));
  ULONG lHashVal=new ULONG(hashVal);
  USHORTByReference pcFound=new USHORTByReference(found);
  HRESULT hr=this.typelib.FindName(szNameBuf,lHashVal,null,null,pcFound);
  COMUtils.checkRC(hr);
  found=pcFound.getValue().shortValue();
  ITypeInfo[] ppTInfo=new ITypeInfo[found];
  MEMBERID[] rgMemId=new MEMBERID[found];
  hr=this.typelib.FindName(szNameBuf,lHashVal,ppTInfo,rgMemId,pcFound);
  COMUtils.checkRC(hr);
  FindName findName=new FindName(szNameBuf.getString(),ppTInfo,rgMemId,found);
  OLEAUTO.SysFreeString(szNameBuf.getValue());
  return findName;
}
