{
  MSWord msWord=null;
  try {
    msWord=new MSWord();
    msWord.setVisible(Variant.VARIANT_TRUE);
  }
 catch (  AutomationException e) {
    e.printStackTrace();
  }
 finally {
    if (msWord != null)     try {
      msWord.quit();
    }
 catch (    AutomationException e) {
      e.printStackTrace();
    }
  }
}
