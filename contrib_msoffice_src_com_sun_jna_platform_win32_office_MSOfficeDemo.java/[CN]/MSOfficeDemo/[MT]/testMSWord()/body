{
  MSWord msWord=null;
  try {
    msWord=new MSWord();
    System.out.println("MSWord version: " + msWord.getVersion());
    msWord.setVisible(Variant.VARIANT_TRUE);
    msWord.newDocument();
    msWord.insertText("Hello from JNA!");
    msWord.closeActiveDocument(Variant.VARIANT_TRUE);
    msWord.quit();
  }
 catch (  COMException e) {
    if (e.getExcepInfo() != null) {
      System.out.println("bstrSource: " + e.getExcepInfo().bstrSource);
      System.out.println("bstrDescription: " + e.getExcepInfo().bstrDescription);
    }
 else     e.printStackTrace();
    if (msWord != null)     msWord.quit();
  }
}
