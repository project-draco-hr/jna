{
  ensureAllocated();
synchronized (reading) {
    if (reading.contains(this))     return;
    reading.add(this);
  }
  try {
    for (Iterator i=structFields.values().iterator(); i.hasNext(); ) {
      readField((StructField)i.next());
    }
  }
  finally {
synchronized (reading) {
      reading.remove(this);
    }
  }
}
