{
  if (address == null) {
    s=null;
  }
 else {
    if (s == null || !address.equals(s.getPointer())) {
      s=newInstance(type);
      s.useMemory(address);
    }
    if (s.getAutoRead()) {
      s.read();
      if (s.array != null) {
        for (int i=1; i < s.array.length; i++) {
          s.array[i].read();
        }
      }
    }
  }
  return s;
}
