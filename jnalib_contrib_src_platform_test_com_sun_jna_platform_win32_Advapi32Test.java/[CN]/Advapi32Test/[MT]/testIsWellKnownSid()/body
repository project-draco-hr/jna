{
  String sidString="S-1-1-0";
  PSIDByReference sid=new PSIDByReference();
  assertTrue(Advapi32.INSTANCE.ConvertStringSidToSid(sidString,sid));
  assertTrue(Advapi32.INSTANCE.IsWellKnownSid(sid.getValue(),WELL_KNOWN_SID_TYPE.WinWorldSid));
  assertFalse(Advapi32.INSTANCE.IsWellKnownSid(sid.getValue(),WELL_KNOWN_SID_TYPE.WinAccountAdministratorSid));
}
