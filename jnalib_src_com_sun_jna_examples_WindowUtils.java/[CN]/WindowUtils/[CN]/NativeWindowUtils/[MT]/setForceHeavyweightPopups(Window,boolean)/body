{
  if (!(w instanceof HeavyweightForcer)) {
    Window[] owned=w.getOwnedWindows();
    for (int i=0; i < owned.length; i++) {
      if (owned[i] instanceof HeavyweightForcer) {
        if (force)         return;
        owned[i].dispose();
      }
    }
    if (force) {
      new HeavyweightForcer(w);
    }
  }
}
