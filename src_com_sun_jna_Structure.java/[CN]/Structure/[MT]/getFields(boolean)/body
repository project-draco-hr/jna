{
  List flist=getFieldList();
  List fieldOrder=fieldOrder();
  if (fieldOrder.size() != flist.size() && flist.size() > 1) {
    if (force) {
      throw new Error("There is an error in Structure.getFieldOrder() on " + getClass());
    }
    return null;
  }
  sortFields(flist,fieldOrder);
  return flist;
}
