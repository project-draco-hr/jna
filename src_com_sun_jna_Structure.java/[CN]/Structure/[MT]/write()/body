{
  ensureAllocated();
  if (this instanceof ByValue) {
    getTypeInfo();
  }
  if (busy().contains(this)) {
    return;
  }
  busy().add(this);
  try {
    for (Iterator i=fields().values().iterator(); i.hasNext(); ) {
      StructField sf=(StructField)i.next();
      if (!sf.isVolatile) {
        writeField(sf);
      }
    }
  }
  finally {
    busy().remove(this);
  }
}
