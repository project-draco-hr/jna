{
  Class type=f.type;
  Object value=getFieldValue(f.field);
  if (typeMapper != null) {
    ToNativeConverter nc=typeMapper.getToNativeConverter(type);
    if (nc != null) {
      type=nc.nativeType();
      value=nc.toNative(value,new ToNativeContext());
    }
  }
  return FFIType.get(value,type);
}
