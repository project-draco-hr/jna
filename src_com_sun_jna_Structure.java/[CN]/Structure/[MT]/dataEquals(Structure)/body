{
  byte[] data=s.getPointer().getByteArray(0,s.size());
  byte[] ref=getPointer().getByteArray(0,size());
  if (data.length == ref.length) {
    for (int i=0; i < data.length; i++) {
      if (data[i] != ref[i]) {
        System.out.println("byte mismatch at offset " + i);
        return false;
      }
    }
    return true;
  }
  return false;
}
