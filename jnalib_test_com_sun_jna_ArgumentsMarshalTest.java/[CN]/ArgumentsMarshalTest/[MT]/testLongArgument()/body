{
  long value=0L;
  assertEquals("Should return 64-bit argument",value,lib.returnInt64Argument(value));
  value=1L;
  assertEquals("Should return 64-bit argument",value,lib.returnInt64Argument(value));
  value=0x7FFFFFFFL;
  assertEquals("Should return 64-bit argument",value,lib.returnInt64Argument(value));
  value=0x80000000L;
  assertEquals("Should return 64-bit argument",value,lib.returnInt64Argument(value));
  value=0x7FFFFFFF00000000L;
  assertEquals("Should return 64-bit argument",value,lib.returnInt64Argument(value));
  value=0x8000000000000000L;
  assertEquals("Should return 64-bit argument",value,lib.returnInt64Argument(value));
}
