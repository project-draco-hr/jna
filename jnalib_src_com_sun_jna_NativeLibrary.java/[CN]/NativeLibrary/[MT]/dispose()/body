{
synchronized (libraries) {
    libraries.remove(getName() + options);
    File path=getFile();
    if (path != null) {
      libraries.remove(path.getAbsolutePath() + options);
      libraries.remove(path.getName() + options);
    }
  }
synchronized (this) {
    if (handle != 0) {
      close(handle);
      handle=0;
    }
  }
}
