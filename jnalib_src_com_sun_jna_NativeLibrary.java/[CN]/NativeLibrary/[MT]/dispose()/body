{
synchronized (libraries) {
    libraries.remove(getName());
    File path=getFile();
    if (path != null) {
      libraries.remove(path.getAbsolutePath());
      libraries.remove(path.getName());
    }
  }
synchronized (this) {
    if (handle != 0) {
      close(handle);
      handle=0;
    }
  }
}
