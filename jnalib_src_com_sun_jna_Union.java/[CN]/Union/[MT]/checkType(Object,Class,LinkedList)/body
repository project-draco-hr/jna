{
  for (Iterator i=fields().values().iterator(); i.hasNext(); ) {
    StructField f=(StructField)i.next();
    if (f.type == type) {
      activeField=f;
      setField(f,object);
      return true;
    }
  }
  Class[] interfaces=type.getInterfaces();
  for (int i=0; i < interfaces.length; i++) {
    interfaceQueue.add(interfaces[i]);
  }
  return false;
}
