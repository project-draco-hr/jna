{
class TestStructure extends Structure {
{
      setFieldOrder(new String[]{"b","s","i","l","f","d"});
    }
    public byte b;
    public short s;
    public int i;
    public long l;
    public float f;
    public double d;
  }
  TestStructure s=new TestStructure();
  s.setAlignType(Structure.ALIGN_MSVC);
  assertEquals("Wrong structure size",32,s.size());
}
