{
  if (AbstractWin32TestSupport.isEnglishLocale) {
    assertEquals("The remote server has been paused or is in the process of being started.",Kernel32Util.formatMessageFromLastErrorCode(W32Errors.ERROR_SHARING_PAUSED));
  }
 else {
    System.out.println("testFormatMessageFromLastErrorCode Test can only be run on english locale");
  }
}
