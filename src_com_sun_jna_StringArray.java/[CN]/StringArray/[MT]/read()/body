{
  boolean returnWide=original instanceof WString[];
  for (int si=0; si < original.length; si++) {
    Pointer p=getPointer(si * Pointer.SIZE);
    Object s=null;
    if (p != null) {
      s=p.getString(0,wide);
      if (returnWide)       s=new WString((String)s);
    }
    original[si]=s;
  }
}
