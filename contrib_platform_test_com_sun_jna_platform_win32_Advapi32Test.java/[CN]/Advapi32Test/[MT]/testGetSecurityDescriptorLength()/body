{
  File file=createTempFile();
  int infoType=OWNER_SECURITY_INFORMATION | GROUP_SECURITY_INFORMATION | DACL_SECURITY_INFORMATION;
  PointerByReference ppSecurityDescriptor=new PointerByReference();
  assertEquals(Advapi32.INSTANCE.GetNamedSecurityInfo(file.getAbsolutePath(),AccCtrl.SE_OBJECT_TYPE.SE_FILE_OBJECT,infoType,null,null,null,null,ppSecurityDescriptor),0);
  assertTrue(Advapi32.INSTANCE.GetSecurityDescriptorLength(ppSecurityDescriptor.getValue()) > 0);
  Kernel32.INSTANCE.LocalFree(ppSecurityDescriptor.getValue());
  file.delete();
}
