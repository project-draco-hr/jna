{
  ensureAllocated();
  for (Iterator i=fields().values().iterator(); i.hasNext(); ) {
    StructField f=(StructField)i.next();
    if (f.type == type) {
      activeField=f;
      read();
      return getFieldValue(activeField.field);
    }
  }
  throw new IllegalArgumentException("No field of type " + type + " in "+ this);
}
